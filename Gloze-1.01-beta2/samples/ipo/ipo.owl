@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ns1:     <http://example.org#> .
@prefix ipo:     <http://www.example.com/IPO> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ipo_:    <http://www.example.com/IPO#> .

ipo_:SKU
      a       owl:Class ;
      rdfs:subClassOf xsd:string .

ipo_:purchaseOrder
      a       owl:ObjectProperty ;
      rdfs:range ipo_:PurchaseOrderType .

ns1:orderDate
      a       rdf:Property , owl:DatatypeProperty .

ipo_:comment
      a       rdf:Property , owl:DatatypeProperty ;
      rdfs:range xsd:string .

ns1:billTo
      a       rdf:Property , owl:ObjectProperty .

ns1:productName
      a       rdf:Property , owl:DatatypeProperty .

ipo_:PurchaseOrderType
      a       owl:Class ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "1"^^xsd:int ;
                owl:onProperty ns1:billTo
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "1"^^xsd:int ;
                owl:onProperty ns1:shipTo
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom ipo_:Items ;
                owl:onProperty ns1:items
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "1"^^xsd:int ;
                owl:onProperty ns1:items
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:int ;
                owl:onProperty ipo_:comment
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom ipo_:Address ;
                owl:onProperty ns1:billTo
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:int ;
                owl:onProperty ns1:orderDate
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom xsd:date ;
                owl:onProperty ns1:orderDate
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom ipo_:Address ;
                owl:onProperty ns1:shipTo
              ] .

ns1:shipTo
      a       rdf:Property , owl:ObjectProperty .

ns1:USPrice
      a       rdf:Property , owl:DatatypeProperty .

ns1:items
      a       rdf:Property , owl:ObjectProperty .

ns1:item
      a       rdf:Property , owl:ObjectProperty .

ns1:partNum
      a       rdf:Property , owl:DatatypeProperty .

ns1:shipDate
      a       rdf:Property , owl:DatatypeProperty .

<http://example.org/ipo>
      a       owl:Ontology ;
      rdfs:comment "International Purchase order schema for Example.com Copyright 2000 Example.com. All rights reserved."@en ;
      owl:imports <http://example.org/address.owl> .

ipo_:Items
      a       owl:Class ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:allValuesFrom
                        [ a       owl:Class ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:maxCardinality "1"^^xsd:int ;
                                    owl:onProperty ipo_:comment
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:maxCardinality "1"^^xsd:int ;
                                    owl:onProperty ns1:shipDate
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:cardinality "1"^^xsd:int ;
                                    owl:onProperty ns1:productName
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:allValuesFrom xsd:date ;
                                    owl:onProperty ns1:shipDate
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:cardinality "1"^^xsd:int ;
                                    owl:onProperty ns1:USPrice
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:allValuesFrom xsd:decimal ;
                                    owl:onProperty ns1:USPrice
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:allValuesFrom xsd:string ;
                                    owl:onProperty ns1:productName
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:cardinality "1"^^xsd:int ;
                                    owl:onProperty ns1:quantity
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:allValuesFrom xsd:positiveInteger ;
                                    owl:onProperty ns1:quantity
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:allValuesFrom ipo_:SKU ;
                                    owl:onProperty ns1:partNum
                                  ] ;
                          rdfs:subClassOf
                                  [ a       owl:Restriction ;
                                    owl:cardinality "1"^^xsd:int ;
                                    owl:onProperty ns1:partNum
                                  ]
                        ] ;
                owl:onProperty ns1:item
              ] .

ns1:quantity
      a       rdf:Property , owl:DatatypeProperty .
